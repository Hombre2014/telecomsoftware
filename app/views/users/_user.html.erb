<tr id="user_row_<%= user.id %>">
  <td class="border border-slate-300 px-3"><%= user.first_name %></td>
  <td class="border border-slate-300 px-3"><%= @step_one == false ? user.last_name : user.duplicates[0] %></td>
  <td class="border border-slate-300 px-3"><%= user.phone %></td>
  <td class="border border-slate-300 px-3">
    <%= link_to 'View', user, class: "rounded-lg py-1 px-5 bg-cyan-500 inline-block font-medium text-white my-3" %>
    <%= link_to 'Edit', edit_user_path(user), class: "rounded-lg py-1 px-5 bg-cyan-500 inline-block font-medium text-white my-3" %>
    <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' }, class: "rounded-lg py-1 px-5 bg-cyan-500 inline-block font-medium text-white my-3" %>
  </td>
</tr>
